apiVersion: v1
kind: Service
metadata:
  name: {{ include "{{ service_name_kebab }}.fullname" . }}
  labels:
    {{- include "{{ service_name_kebab }}.labels" . | nindent 4 }}
spec:
  type: {{ .Values.service.type }}
  ports:
    - port: {{ .Values.service.port }}
      targetPort: http
      protocol: TCP
      name: http
  selector:
    {{- include "{{ service_name_kebab }}.selectorLabels" . | nindent 4 }}